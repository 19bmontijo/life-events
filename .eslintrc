{
	"extends": [
		"airbnb-base",
		"standard"
	],
	"env": {
		"es6": true,
		"browser": true,
		"node": true,
		"mocha": true,
		"jest": true
	},
	"plugins": [
		"standard",
		"babel",
		"import"
	],
	"globals": {
		"state": true
	},
	"rules": {
		"import/extensions": 0,
		"indent": [
			2,
			"tab"
		],
		"no-tabs": 0,
		"semi": 0
	}
}
